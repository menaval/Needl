<% block.each do |name,ids|%>
  <div class="row">
    <div class= "col-xs-4">
      <%= image_tag "#{name}.svg", class: "icon"%>
      <h3><%= name.capitalize.gsub(/-/," ").gsub(/&/, "and") %></h3>
    </div>
    <div class= "col-xs-8">
      <ul class="list-inline">
       <% ids.each do |id| %>
        <li>
          <%= image_tag User.find(id.to_i).picture.url(:thumb), class: "avatar img-circle img-responsive" %>
        </li>
       <% end %>
      </ul>
    </div>
  </div>
<% end %>